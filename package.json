    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "next-safe-action": "^7.10.8",
    "nextjs-routes": "^2.2.5",
    "nodemailer": "^6.10.1",
    "oslo": "^1.2.1",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "safe-stable-stringify": "^2.5.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.3"
