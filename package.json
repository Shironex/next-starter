{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier src/ --write",
    "typecheck": "tsc --noEmit",
    "db:push": "dotenv -e .env.local drizzle-kit push",
    "db:generate": "dotenv -e .env.local drizzle-kit generate ",
    "db:migrate": "dotenv -e .env.local tsx src/lib/db/migrate.ts",
    "db:studio": "dotenv -e .env.local drizzle-kit studio",
    "db:setup": "pnpm db:generate && pnpm db:migrate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-email/components": "^0.0.28",
    "@react-email/render": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.11.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.1",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.453.0",
    "next": "14.2.15",
    "nextjs-routes": "^2.2.3",
    "nodemailer": "^6.9.16",
    "oslo": "^1.2.1",
    "postgres": "^3.4.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.28.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-n": "^17.11.1",
    "jiti": "^2.3.3",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "license": "MIT"
}
